<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashScren">

    <fragment
        android:id="@+id/splashScren"
        android:name="com.etibe.app.SplashScren"
        android:label="fragment_splash_scren"
        tools:layout="@layout/fragment_splash_scren" >
        <action
            android:id="@+id/action_splashScren_to_onboarding"
            app:destination="@id/onboarding" />
    </fragment>
    <fragment
        android:id="@+id/onboarding"
        android:name="com.etibe.app.Onboarding"
        android:label="fragment_onboarding"
        tools:layout="@layout/fragment_onboarding" >
        <action
            android:id="@+id/action_onboarding_to_login2"
            app:destination="@id/login" />
    </fragment>
    <fragment
        android:id="@+id/onboardingPageFragment"
        android:name="com.etibe.app.OnboardingPageFragment"
        android:label="fragment_onboarding_page"
        tools:layout="@layout/fragment_onboarding_page" >
        <action
            android:id="@+id/action_onboardingPageFragment_to_login"
            app:destination="@id/login" />
    </fragment>
    <fragment
        android:id="@+id/login"
        android:name="com.etibe.app.Login"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" />
</navigation>